{
  "name": "13-street-fee",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build-watch": "parcel watch './public/**/*.html' --public-url / --dist-dir dist/",
    "start-watch": "nodemon ./bin/www",
    "dev": "concurrently --kill-others \"npm run start-watch\" \"npm run build-watch\"",
    "build": "parcel build './public/**/*.html' --no-scope-hoist --no-source-maps  --no-optimize",
    "start": "npm run build && node ./bin/www"
  },
  "dependencies": {
    "@picmo/popup-picker": "^5.8.5",
    "axios": "^1.2.1",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.2",
    "cookie-parser": "~1.4.4",
    "cropperjs": "^1.6.1",
    "debug": "~2.6.9",
    "emojibase-data": "^15.3.2",
    "express": "^4.18.2",
    "http-errors": "~1.6.3",
    "jquery": "^3.6.1",
    "jquery-ui": "^1.13.2",
    "morgan": "~1.9.1",
    "pug": "^3.0.2",
    "sticky-js": "^1.3.0",
    "vue": "^3.4.23",
    "vue-easy-dnd": "^2.1.3",
    "vue-search-select": "^3.2.0"
  },
  "devDependencies": {
    "@parcel/compressor-gzip": "^2.11.0",
    "@parcel/packager-raw-url": "^2.12.0",
    "@parcel/transformer-jsonld": "^2.12.0",
    "@parcel/transformer-sass": "^2.11.0",
    "assert": "^2.1.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^5.7.1",
    "concurrently": "^7.6.0",
    "crypto-browserify": "^3.12.0",
    "events": "^3.3.0",
    "nodemon": "^3.0.1",
    "parcel": "^2.11.0",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "parcel-resolver-ignore": "^2.1.3",
    "parcel-resolver-website": "^1.0.1",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.1",
    "util": "^0.12.5",
    "vm-browserify": "^1.1.2"
  },
  "alias": {
    "vue": "vue/dist/vue.esm-browser.js"
  },
  "staticFiles": {
    "staticPath": "public/favicon.png"
  }
}
