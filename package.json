{
  "name": "13-street-fee",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build-watch": "parcel watch './public/**/*.html' --public-url / --dist-dir dist/",
    "start-watch": "nodemon ./bin/www",
    "dev": "concurrently --kill-others \"npm run start-watch\" \"npm run build-watch\"",
    "build": "parcel build './public/**/*.html'",
    "start": "npm run build && node ./bin/www"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jquery": "^3.6.1",
    "jquery-ui": "^1.13.2",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11",
    "sticky-js": "^1.3.0",
    "vue": "^3.2.36"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.8.1",
    "buffer": "^5.7.1",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "parcel": "^2.8.1",
    "parcel-resolver-ignore": "^2.1.3",
    "parcel-resolver-website": "^1.0.1"
  }
}
